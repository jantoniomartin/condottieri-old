#, python-format
msgid "%(unit)s besieges the city."
msgstr "%(unit)s asedia la ciudad."

#: models.py:153
#, python-format
msgid "%(unit)s tries to convert into %(type)s."
msgstr "%(unit)s intenta convertirse en %(type)s."

#: models.py:158
#, python-format
