#: models.py:30
#, python-format
msgid "the army in %s"
msgstr "el ejército en %s"

#: models.py:32
#, python-format
msgid "the fleet in %s"
msgstr "la flota en %s"

#: models.py:34
#, python-format
msgid "the garrison in %s"
msgstr "la guarnición en %s"

#: models.py:78
#, python-format
msgid "%(country)s recruits a new %(type)s in %(area)s."
msgstr "%(country)s recluta %(type)s en %(area)s."

#: models.py:103
#, python-format
msgid "%(country)s's %(type)s in %(area)s is disbanded."
msgstr "%(type)s de %(country)s en %(area)s es desbandada."

#: models.py:109
#, python-format
msgid "Autonomous %(type)s in %(area)s is disbanded."
msgstr "%(type)s autónoma en %(area)s es desbandada."

#: models.py:146
#, python-format
msgid "%(unit)s tries to go to %(area)s."
msgstr "%(unit)s intenta ir a %(area)s."

